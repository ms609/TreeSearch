begin mrbayes;

  set autoclose=yes nowarnings=yes;
  
  prset statefreqpr=fixed(equal); [Jukes-Cantor model]
  prset brlenspr=unconstrained:uniform(0,3);
  prset tratiopr=fixed(1);
  
  mcmcp nruns=2 ngen=5000000 samplefreq=200 nchains=8 burninfrac=0.5;
  mcmcp printfreq=10000 diagnfreq=50000 printmax=4 savebrlens=no;
  mcmcp mcmcdiagn=yes stoprule=yes stopval=0.005;
  
  mcmc;
  sump;
  sumt;
  
end;
